<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <!--
    In code, please prefer annotations as a way of ignoring Findbugs issues
    -->

    <!-- Exclude test classes -->
    <Match>
        <Class name="~.*Test$"/>
    </Match>
    <Match>
        <Class name="~.*Tests$"/>
    </Match>
    <Match>
        <Package name="~test\..*"/>
    </Match>
    <Match>
        <Package name="~io.realm.entities.*"/>
    </Match>

    <!-- Generated code -->
    <Match>
        <Class name="~.*RealmProxy$"/>
    </Match>
</FindBugsFilter>
