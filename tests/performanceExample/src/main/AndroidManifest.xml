<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.realm.examples.performance" >

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:name=".sugar_orm.SugarApplication"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".PerformanceExampleActivity"
            android:label="Performance Comparisons" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ContainerActivity"
            android:label="Performance Comparisons" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data android:name="DATABASE"            android:value="EmployeeSugar.db" />
        <meta-data android:name="VERSION"             android:value="0.01" />
        <meta-data android:name="QUERY_LOG"           android:value="true" />
        <meta-data android:name="DOMAIN_PACKAGE_NAME" android:value="io.realm.examples.performance" />
    </application>

</manifest>
