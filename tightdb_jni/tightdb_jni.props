<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <tightdb_path>..\..\..\tightdb\</tightdb_path>
    <outputdir>..\lib\</outputdir>
  </PropertyGroup>
  <PropertyGroup>
    <OutDir>$(outputdir)</OutDir>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <Link>
      <AdditionalLibraryDirectories>$(tightdb_path)lib</AdditionalLibraryDirectories>
      <ManifestFile>$(IntDir)$(TargetName)$(TargetExt).intermediate.manifest</ManifestFile>
    </Link>
    <ClCompile>
      <AdditionalIncludeDirectories>$(tightdb_path);$(tightdb_path)src\;$(tightdb_path)src\win32\pthread\;$(JAVA_INCLUDE);</AdditionalIncludeDirectories>
      <MultiProcessorCompilation>true</MultiProcessorCompilation>
      <AssemblerListingLocation>$(IntDir)\</AssemblerListingLocation>
      <ObjectFileName>$(IntDir)\</ObjectFileName>
      <ProgramDataBaseFileName>$(IntDir)\vc$(PlatformToolsetVersion).pdb</ProgramDataBaseFileName>
      <XMLDocumentationFileName>$(IntDir)\</XMLDocumentationFileName>
      <BrowseInformationFile>$(IntDir)\</BrowseInformationFile>
      <FunctionLevelLinking>true</FunctionLevelLinking>
    </ClCompile>
  </ItemDefinitionGroup>
  <ItemGroup>
    <BuildMacro Include="tightdb_path">
      <Value>$(tightdb_path)</Value>
    </BuildMacro>
    <BuildMacro Include="outputdir">
      <Value>$(outputdir)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>