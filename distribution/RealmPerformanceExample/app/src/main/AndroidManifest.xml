<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.realm.examples.performance" >

    <application
        android:allowBackup="true"
        android:name=".SugarApplication"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".sqlite.SQLiteTestActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".realm.RealmTestActivity"
            android:label="RealmTestActivity" >
        </activity>
        <activity
            android:name=".sugar_orm.SugarORMTestActivity"
            android:label="SugarORMTestActivity" >
        </activity>
        <activity
            android:name=".ormlite.ORMLiteTestActivity"
            android:label="ORMLiteTestActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

    <meta-data android:name="DATABASE"            android:value="sugarorm.db" />
    <meta-data android:name="VERSION"             android:value="0.01" />
    <meta-data android:name="QUERY_LOG"           android:value="true" />
    <meta-data android:name="DOMAIN_PACKAGE_NAME" android:value="io.realm.examples.performance" />

</manifest>
